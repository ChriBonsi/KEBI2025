@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:crescia a ex:Dish ;
    ex:hasIngredient ex:acqua,
        ex:farina,
        ex:olio,
        ex:rosmarino .

ex:hasCalories a owl:DatatypeProperty ;
    rdfs:domain ex:Ingredient ;
